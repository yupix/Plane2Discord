{
  "name": "plane2discord",
  "version": "0.1.0",
  "description": "Forward webhooks to Discord",
  "main": "src/server.ts",
  "scripts": {
    "start": "deno run --allow-net --allow-read --allow-env --env-file --allow-sys --allow-write src/server.ts",
    "dev": "deno run --allow-net --allow-read --allow-env --env-file --allow-sys --allow-write --watch src/server.ts"
  },
  "dependencies": {
    "@discordeno/bot": "^21.0.0"
  },
  "devDependencies": {}
}
